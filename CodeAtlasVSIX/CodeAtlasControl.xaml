<UserControl x:Class="CodeAtlasVSIX.CodeAtlasControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="MyToolWindow">
    <Grid>
        <Canvas x:Name="canvas" Background="#FF464141" MouseWheel="onMouseWheel" ClipToBounds="True">
            <Button x:Name="button" Content="Button" Canvas.Left="84" Canvas.Top="68" Width="75" Click="button_Click"/>
        </Canvas>
    </Grid>
</UserControl>
