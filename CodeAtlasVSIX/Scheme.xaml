<UserControl x:Class="CodeAtlasVSIX.Scheme"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CodeAtlasVSIX"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="13"/>
            <RowDefinition Height="13"/>
            <RowDefinition Height="13"/>
            <RowDefinition Height="13"/>
            <RowDefinition Height="13"/>
            <RowDefinition Height="13"/>
            <RowDefinition Height="13"/>
        </Grid.RowDefinitions>
        <Button x:Name="schemeButton0" Content="+" Grid.RowSpan="1" Grid.Row="0" Margin="0" Grid.Column="1" FontSize="8" />
        <Button x:Name="schemeButton1" Content="+" Grid.RowSpan="1" Grid.Row="1" Margin="0" Grid.Column="1" FontSize="8" />
        <Button x:Name="schemeButton2" Content="+" Grid.RowSpan="1" Grid.Row="2" Margin="0" Grid.Column="1" FontSize="8" />
    </Grid>
</UserControl>
